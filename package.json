{
  "name": "SpillForWork",
  "version": "1.4.4",
  "private": true,
  "scripts": {
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "assemble:android:prod": "cd android && ENVFILE=.env.prod ./gradlew assembleProdRelease && cd ..",
    "assemble:android:staging": "cd android && ENVFILE=.env.staging ./gradlew assembleStagingRelease && cd ..",
    "android:bundle": "ENVFILE=.env.prod react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:dev": "ENVFILE=.env.dev react-native run-android --variant=devDebug",
    "android:prod": "ENVFILE=.env.prod react-native run-android --variant=prodDebug",
    "android:staging": "ENVFILE=.env.staging react-native run-android --variant=stagingDebug",
    "ios:dev": "ENVFILE=.env.dev react-native run-ios --scheme Spill-dev",
    "ios:staging": "ENVFILE=.env.staging react-native run-ios --scheme Spill-staging",
    "ios:prod": "ENVFILE=.env.prod react-native run-ios --scheme Spill-prod",
    "prestorybook": "rnstl",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "storybook": "storybook start -p 7007",
    "test": "jest"
  },
  "dependencies": {
    "@pusher/chatkit": "^0.7.12",
    "appcenter": "^1.8.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "buffer": "^5.2.1",
    "metro-react-native-babel-preset": "^0.45.3",
    "react": "^16.5.2",
    "react-native": "^0.57.0",
    "react-native-check-box": "^2.1.0",
    "react-native-code-push": "^5.4.2",
    "react-native-config": "^0.11.5",
    "react-native-firebase": "^4.2.0",
    "react-native-hyperlink": "0.0.12",
    "react-native-indicators": "^0.13.0",
    "react-native-mixpanel": "^1.1.1",
    "react-native-progress": "^3.5.0",
    "react-native-sentry": "^0.39.0",
    "react-native-svg": "^6.5.2",
    "react-native-swiper": "^1.5.13",
    "react-native-vector-icons": "^4.6.0",
    "react-native-video": "^2.2.0",
    "react-navigation": "^2.1.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "sendbird": "3.0.45",
    "styled-components": "^3.3.3"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/",
      "./assets/images/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react-native": "^3.4.8",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "jest": "^23.2.0",
    "jest-serializer-enzyme": "^1.0.0",
    "jest-styled-components": "^5.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.1",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "^16.4.1",
    "redux-logger": "^3.0.6",
    "schedule": "^0.4.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "styled-components": "<rootDir>/node_modules/styled-components/dist/styled-components.native.cjs.js"
    },
    "setupFiles": [
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "./components/**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
